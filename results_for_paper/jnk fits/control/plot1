unset multiplot

set term 'png' size 2600,1200
set output 'dr_contr.png'
set key bottom right

set multiplot layout 2,3 title ""

set ylabel "MKK4"
plot "dr_contr.exp" using 1:2 title "dr_contr_exp" with points ps 2 pt 5 , \
"dr_contr_ANI.scan" using 1:48 title "dr_contr_sim" with lines lw 2

set ylabel "MKK7"
plot "dr_contr.exp" using 1:3 title "dr_contr_exp" with points ps 2 pt 5 , \
"dr_contr_ANI.scan" using 1:49 title "dr_contr_sim" with lines lw 2

set ylabel "JNK"
plot "dr_contr.exp" using 1:4 title "dr_contr_exp" with points ps 2 pt 5 , \
"dr_contr_ANI.scan" using 1:50 title "dr_contr_sim" with lines lw 2



set ylabel "MKK4"
plot "dr_JNKinh.exp" using 1:2 title "dr_JNKinh_exp" with points ps 2 pt 5 , \
"dr_JNKinh_ANI.scan" using 1:48 title "dr_JNKinh_sim" with lines lw 2

set ylabel "MKK7"
plot "dr_JNKinh.exp" using 1:3 title "dr_JNKinh_exp" with points ps 2 pt 5 , \
"dr_JNKinh_ANI.scan" using 1:49 title "dr_JNKinh_sim" with lines lw 2

set ylabel "JNK"
plot "dr_JNKinh.exp" using 1:4 title "dr_JNKinh_exp" with points ps 2 pt 5 , \
"dr_JNKinh_ANI.scan" using 1:50 title "dr_JNKinh_sim" with lines lw 2

unset multiplot
